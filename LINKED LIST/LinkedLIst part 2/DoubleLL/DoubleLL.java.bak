class Node
{
	int data;
	Node next;
	Node prev;
	public Node(int data){
		this.data=data;
		this.next=null;
		this.prev=null;
	}
}
class DoubleLL 

{
	public static Node head;
	public static Node tail;
	public static int size;
	public void addFirst(int data){
		Node newnode =new Node(data);
		size++;
		if (head==null)
		{
			head=tail=newnode;
			return;

		}
		newnode.next=head;
		head.prev=newnode;
		head=newnode;
	}
	public void printnode(){
		Node temp=head;
		while (temp != null)
		{
			System.out.print(temp.data+"==>");
			temp=temp.next;

		}
		System.out.println();
	}
	public static void main(String[] args) 
	{
		DoubleLL dll=new DoubleLL();
		dll.addFirst(5);
		dll.addFirst(7);
		dll.addFirst(2);
		dll.printnode();
		System.out.println(dll.size);
	}
}
